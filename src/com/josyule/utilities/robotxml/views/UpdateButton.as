package com.josyule.utilities.robotxml.views {	import fl.controls.Button;	import flash.display.Sprite;	import flash.events.MouseEvent;			public class UpdateButton extends Sprite 	{				private var update:Button;				//--------------------------------------------------------------------------		//		//  Initialization		//		//--------------------------------------------------------------------------		/**		 * Event handling takes place via the mediator: UpdateButtonMediator 		 * 		 */				public function UpdateButton() 		{						setupUI();		}				public function set enabled(bool:Boolean):void		{			update.enabled = bool;		}				protected function setupUI():void		{			update = new Button;			update.label = "Update";			update.addEventListener(MouseEvent.CLICK, updateClickEvent, false, 0, true);			update.x = 10;			update.y = 10;			addChild(update);			update.enabled = false;		}				protected function updateClickEvent(e:MouseEvent):void		{			trace("start the update process!");		}					}}