package com.josyule.utilities.robotxml.views {		import RobotXML;		import org.robotlegs.mvcs.Mediator;	import org.robotlegs.base.ContextEvent;		public class RobotXMLMediator extends Mediator 	{				// declare the view to be injected		[Inject]		public var view:RobotXML;				//--------------------------------------------------------------------------		//		//  Initialization		//		//--------------------------------------------------------------------------		/**		 * Avoid doing work in the constructor!		 * onRegister() is the place to do things. 		 * 		 */				public function RobotXMLMediator() 		{						// Try not to put stuff in the constructor of the mediator		}				//--------------------------------------------------------------------------		//		//  Overridden API		//		//--------------------------------------------------------------------------		/**		 * Initialize the view and register for events. 		 * 		 */		override public function onRegister():void		{						trace("RobotXMLMediator.onRegister()");			//eventMap.mapListener(eventDispatcher, EventType.EVENT_NAME, eventHandlerFunction, eventClass);			eventMap.mapListener(eventDispatcher, ContextEvent.STARTUP_COMPLETE, startupCompleteEvent);		}				//--------------------------------------------------------------------------		//		//  Event Handlers		//		//--------------------------------------------------------------------------				protected function startupCompleteEvent(e:ContextEvent):void		{			view.setupUI();		}	}}